<fest:template xmlns:fest="http://fest.mail.ru">
	<div class="b-game-page b-default-hidden-page">

	    <div class="b-game-page__winner">
	        <span>Winner: </span><span class="b-game-page__winner__winner-name"></span>
	    </div>
	    <div class="b-game-page__gameplay">
	    	<button class="b-game-page__start-button">start</button>
	    </div>
	    <div class="audio">
			<button class="audio__toggle-recording">Start!</button>
			<span class="audio__herz-indicator">--</span>
			<canvas class="audio__wave-canvas" id="track"></canvas>			
		</div>
    </div>
</fest:template>
